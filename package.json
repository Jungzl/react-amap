{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "funding": "https://jaywcjlove.github.io/#/sponsor",
  "homepage": "https://uiwjs.github.io/react-amap",
  "scripts": {
    "prepare": "husky install",
    "build": "pnpm -F @uiw/* build",
    "bundle": "pnpm -F @uiw/react-amap bundle",
    "bundle:watch": "pnpm -F @uiw/react-amap bundle:watch",
    "bundle:min": "pnpm -F @uiw/react-amap bundle:min",
    "⬇️⬇️⬇️⬇️⬇️ watch ⬇️⬇️⬇️⬇️⬇️": "▼▼▼▼▼ watch::package ▼▼▼▼▼",
    "watch:amap": "pnpm -F *-amap watch",
    "watch:api-loader": "pnpm -F *-api-loader watch",
    "watch:auto-complete": "pnpm -F *-auto-complete watch",
    "watch:bezier-curve": "pnpm -F *-bezier-curve watch",
    "watch:circle": "pnpm -F *-circle watch",
    "watch:circle-marker": "pnpm -F *-circle-marker watch",
    "watch:context-menu": "pnpm -F *-context-menu watch",
    "watch:control-bar-control": "pnpm -F *-control-bar-control watch",
    "watch:ellipse": "pnpm -F *-ellipse watch",
    "watch:geolocation": "pnpm -F *-geolocation watch",
    "watch:hawk-eye-control": "pnpm -F *-hawk-eye-control watch",
    "watch:info-window": "pnpm -F *-info-window watch",
    "watch:map": "pnpm -F *-map watch",
    "watch:map-type-control": "pnpm -F *-map-type-control watch",
    "watch:tile-layer": "pnpm -F *-tile-layer watch",
    "watch:marker": "pnpm -F *-marker watch",
    "watch:label-marker": "pnpm -F *-label-marker watch",
    "watch:mass-marks": "pnpm -F *-mass-marks watch",
    "watch:mouse-tool": "pnpm -F *-mouse-tool watch",
    "watch:poly-editor": "pnpm -F *-poly-editor watch",
    "watch:polygon": "pnpm -F *-polygon watch",
    "watch:polygon-editor": "pnpm -F *-polygon-editor watch",
    "watch:polyline": "pnpm -F *-polyline watch",
    "watch:polyline-editor": "pnpm -F *-polyline-editor watch",
    "watch:rectangle": "pnpm -F *-rectangle watch",
    "watch:require-script": "pnpm -F *-require-script watch",
    "watch:scale-control": "pnpm -F *-scale-control watch",
    "watch:text": "pnpm -F *-text watch",
    "watch:tool-bar-control": "pnpm -F *-tool-bar-control watch",
    "watch:utils": "pnpm -F *-utils watch",
    "watch:weather": "pnpm -F *-weather watch",
    "⬆️⬆️⬆️⬆️⬆️ watch ⬆️⬆️⬆️⬆️⬆️": "▲▲▲▲▲ watch::package ▲▲▲▲▲",
    "type-check": "pnpm -F @uiw/* exec \"tsc --noEmit\"",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage --bail",
    "version": "lerna version --exact --force-publish --no-push --no-git-tag-version",
    "doc:build": "pnpm -F website build",
    "start": "pnpm -F website start",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "remove": "pnpm -r exec \"rimraf node_modules\"",
    "clean:dist": "pnpm -r exec \"rimraf build dist esm cjs\""
  },
  "author": "Kenny Wong <wowohoo@qq.com>",
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/*.{ts,tsx}",
      "<rootDir>/packages/**/__tests__/*.{ts,tsx}"
    ],
    "collectCoverageFrom": [
      "<rootDir>/packages/**/*.{tsx,ts}",
      "!**/.kktrc.ts",
      "!**/*.{js,d.ts}"
    ]
  },
  "devDependencies": {
    "@babel/runtime": "^7.18.9",
    "@kkt/less-modules": "^7.5.2",
    "@kkt/ncc": "^1.0.15",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "@types/react-test-renderer": "^18.0.0",
    "cross-env": "~7.0.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "lerna": "^8.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.0.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.5",
    "tsbb": "^4.1.14"
  }
}
